        <actionsColumn name="actions" class="Admin\Console\Ui\Component\Listing\ModuleList\Column\Action">
			<settings>
				<indexField>module</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>




      

     





      
    </columns>

</listing>